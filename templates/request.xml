<?xml version="1.0" encoding="UTF-8"?>
<osm-script>
    <union>
        <query type="way">
            <has-kv k="building" v="yes" />
            <bbox-query s="<%= south %>" w="<%= west %>" n="<%= north %>" e="<%= east %>"/>
        </query>
        <query type="way">
            <has-kv k="building" />
            <has-kv k="building:levels" modv="not" regv="[0-9]" />
            <bbox-query s="<%= south %>" w="<%= west %>" n="<%= north %>" e="<%= east %>"/>
        </query>
    </union>
    <union>
        <item/>
        <recurse type="down"/>
    </union>
    <print mode="meta"/>
</osm-script>
